<?xml version="1.0" encoding="UTF-8"?>

<p3dshader>
<node name="Shader">
<input type="vec4" name="Position">
  <main>gl_Position = %i;<br/></main>
</input>
<output name="CODE" type="shader">
<declaration>
#version 330<br/>
#define vshader<br />
</declaration>
<main>
<br/>
void main() {<br/>
  <input name="Position"/><br/>
}<br/>
</main>
</output>
</node>

<node name="Fragment Shader">
<input type="vec4" name="Color">
<main>FragColor = %i;</main>
<declaration>out vec4 FragColor;<br/></declaration>
</input>
<output name="CODE" type="shader">
<declaration>
#version 330<br/>
#define fshader<br />
<input name="Color" />
</declaration>
<main>
<br/>
void main() {<br/>
  <input name="Color"/><br/>
}<br/>
</main>
</output>
</node>

</p3dshader>
