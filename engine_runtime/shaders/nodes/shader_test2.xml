<?xml version="1.0" encoding="UTF-8"?>
<p3dshader>
  <default_shader X="10" Y="150">
    <:input name="Position">
      <operation_multiply X="250" Y="50">
        <:input name="a">
          <operation_multiply X="450" Y="50">
            <:input name="a">
              <operation_multiply X="650" Y="50">
                <:input name="a">
                  <uniform_matproj X="800" Y="50"/>
                </:input>
                <:input name="b">
                  <uniform_matview X="800" Y="450"/>
                </:input>
              </operation_multiply>
            </:input>
            <:input name="b">
              <uniform_matworld X="500" Y="450"/>
            </:input>
          </operation_multiply>
        </:input>
        <:input name="b">
          <attribute_position X="500" Y="650"/>
        </:input>
      </operation_multiply>
    </:input>
    <:input name="Color">    
      <operation_multiply X="650" Y="50">
        <:input name="a">
          <attribute_color X="250" Y="350"/>
        </:input>
        <:input name="b">
          <lib_simpleshading X="800" Y="450">
            <:input name="Normal">
              <operation_multiply X="250" Y="50">
                <:input name="a">
                  <operation_multiply X="450" Y="50">
                    <:input name="a">
                      <operation_multiply X="650" Y="50">
                        <:input name="a">
                          <uniform_matproj X="800" Y="50"/>
                        </:input>
                        <:input name="b">
                          <uniform_matview X="800" Y="450"/>
                        </:input>
                      </operation_multiply>
                    </:input>
                    <:input name="b">
                      <uniform_matworld X="500" Y="450"/>
                    </:input>
                  </operation_multiply>
                </:input>
                <:input name="b">
                  <attribute_normal X="500" Y="650"/>
                </:input>
              </operation_multiply>
            </:input>
          </lib_simpleshading>
        </:input>
      </operation_multiply>
    </:input>
  </default_shader>
</p3dshader>
