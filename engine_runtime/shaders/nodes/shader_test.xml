<?xml version="1.0" encoding="UTF-8"?>
<p3dshader>
  <default_shader X="10" Y="150">
    <:input name="Position">
      <operation_multiply X="250" Y="50">
        <:input name="a">
          <attribute_normal X="500" Y="50"/>
        </:input>
        <:input name="b">
          <attribute_position X="500" Y="450"/>
        </:input>
      </operation_multiply>
    </:input>
    <:input name="Color">
      <attribute_color X="250" Y="350"/>
    </:input>
  </default_shader>
</p3dshader>
